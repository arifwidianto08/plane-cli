# Plane.so API Configuration
# Copy this file to .env and fill in your values

# Your Plane instance URL (e.g., https://plane.your-domain.com)
PLANE_BASE_URL=

# Your Personal Access Token from Plane settings
PLANE_API_TOKEN=

# Optional: Default project identifier
PLANE_DEFAULT_PROJECT=

# Optional: Request timeout in seconds (default: 30)
PLANE_REQUEST_TIMEOUT=30
